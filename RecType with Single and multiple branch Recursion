public class RecType {
    static void singleBranch(int num){
        if (num==0){
            return;
        }
        
    System.out.println(num);
    singleBranch(num-1);
}


    static void multiBranch(int num){
        if (num<=0){
            return;
              }
        System.err.println(num);
        multiBranch(num-1);
        multiBranch(num-2);
    }
    public static void main(String[]args){
        //singleBranch(4);
        multiBranch(4);
    }
}
