public class Fibonacci {
    public static int fib (int n){
        //base case
        if(n==0 || n==1){
            return n;
        }
        
       int result = fib(n-1) + fib(n-2);
       return result;
    }

    public static void main(String[] args) {
        int r= fib(5);
        System.out.println(r);
    }
}

